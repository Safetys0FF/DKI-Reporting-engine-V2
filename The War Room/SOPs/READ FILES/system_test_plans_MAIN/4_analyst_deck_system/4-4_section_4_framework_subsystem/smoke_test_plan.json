{
  "test_plan": {
    "system_name": "Section 4 Framework Subsystem",
    "system_address": "4-4",
    "parent_system": "Analyst Deck (4-x)",
    "test_type": "smoke_test",
    "version": "1.0",
    "created": "2025-10-05",
    "description": "Basic functionality validation for Section 4 Framework (Session Review) subsystem",
    "test_data_location": "./test_data/",
