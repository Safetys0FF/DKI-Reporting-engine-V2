    "performance_metrics": {
      "initialization_time_max_ms": 10000,
      "dependency_resolution_time_max_ms": 15000,
      "dependency_validation_time_max_ms": 20000,
      "dependency_management_time_max_ms": 15000,
      "memory_usage_max_mb": 150,
